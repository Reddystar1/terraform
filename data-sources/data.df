data "aws_ami" "joindevops"{
    owners = ["973714476881"]
    most_recent = true

    filter {
        Name = "name"
        values = ["RHEL-9-DevOps-Practice"]
    }

    filter {
        Name = "root-device-type"
        values = ["EBS"]
    }

    filter {
        Name = "virtualization-type"
        values = ["hvm"]
    }

}    

output "ami_id" {
value = data.aws_ami.joindevops.id  
}
     